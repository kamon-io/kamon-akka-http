<!DOCTYPE aspectj PUBLIC "-//AspectJ//DTD//EN" "http://www.eclipse.org/aspectj/dtd/aspectj.dtd">

<aspectj>
  <aspects>
    <!-- Akka Http Server -->
    <aspect name="akka.kamon.http.instrumentation.ServerRequestInstrumentation"/>

    <!-- Akka Http Client -->
    <aspect name="akka.kamon.http.instrumentation.ClientRequestInstrumentation"/>
  </aspects>

  <weaver options="-warn:none">
    <include within="akka..*"/>
  </weaver>
</aspectj>
